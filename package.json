{
  "name": "pondsoocket",
  "version": "0.1.140",
  "description": "PondSocket is a fast simple socket server",
  "keywords": [
    "socket",
    "server",
    "ws",
    "websocket",
    "PUB/SUB",
    "pubsub",
    "presence",
    "realtime",
    "realtime server"
  ],
  "main": "bin/server.js",
  "types": "types/server.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "commit": "git-cz",
    "setup": "npm install --force && npm run build",
    "build:local": "tsc -p tsconfig.json",
    "coverage": "jest --coverage"
  },
  "exports": {
    ".": "./bin/server.js",
    "./client": "./bin/client.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./types/server.d.ts"
      ],
      "client": [
        "./types/client.d.ts"
      ]
    }
  },
  "author": "Roy OSSAI",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eleven-am/pondSocket.git"
  },
  "dependencies": {
    "ws": "^8.8.1",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/jest": "^28.1.7",
    "@types/node": "^16.10.3",
    "@types/ws": "^8.5.3",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.0.1",
    "parcel": "^2.7.0",
    "semantic-release": "^19.0.5",
    "supertest": "^6.2.4",
    "superwstest": "^2.0.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
